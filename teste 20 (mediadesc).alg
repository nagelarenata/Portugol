algoritmo "mediadesc"
// Função :
// Autor :
// Data : 07/01/2022
// Seção de Declarações 
var
   n1, n2, n3, media, x: real
inicio
// Seção de Comandos
   Escreva("Nota 1: ")
   Leia(n1)
   Escreva("Nota 2: ")
   Leia(n2)
   Escreva("Nota 3: ")
   Leia(n3)
   media <- (n1 + n2 + n3)/3
   Escreva("A sua média é", media)
fimalgoritmo
