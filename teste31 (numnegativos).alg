algoritmo "numnegativos"
// Função :
// Autor :
// Data : 14/01/2022
// Seção de Declarações 
var
   N, C, TotN: inteiro
inicio
// Seção de Comandos
   C <- 1
   TotN <- 0
   Repita
   Escreva("Digite um número: ")
   Leia(N)
   Se (N<0) entao
      TotN <- TotN + 1
   FimSe
   C <- C+1
   Ate(C>5)
   EscrevaL("Foram digitados", TotN, " valores negativos")
fimalgoritmo
